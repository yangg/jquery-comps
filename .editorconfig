# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[{package.json,gulpfile.js}]
indent_size=2

[*.{scss,less}]
indent_size=2

[static/lib/*]
indent_size=2
